      </category>
      <category name="fortition">
         <tag name="fortition"/>
         <description>strengthening</description>
      </category>
   </annotation-set>
   <annotation-set exmaralda-tier-category="BOR-Morph">
      <category name="Morphological adaptation strategies">
         <category name="direct insertion, not inflected">
            <tag name="dir:bare"/>
            <description>insertion without any morphological adaptation, not inflected in matrix language</description>
         </category>
         <category name="direct insertion, further inflected">
            <tag name="dir:infl"/>
            <description>insertion without any morphological adaptation, further inflected in matrix language</description>
         </category>
         <category name="indirect insertion, not inflected">
            <tag name="indir:bare"/>
            <description>insertion with morphological adaptation, not inflected in matrix language</description>
         </category>
         <category name="indirect insertion, further inflected">
            <tag name="indir:infl"/>
            <description>insertion with morphological adaptation, further inflected in matrix language</description>
         </category>
         <category name="paradigm insertion, not inflected">
            <tag name="parad:bare"/>
            <description>a (subset of) paradigm is borrowed with inflexion from the donor language, not inflected in matrix language</description>
         </category>
         <category name="paradigm insertion, further inflected">
            <tag name="parad:infl"/>
            <description>a (subset of) paradigm is borrowed with inflexion from the donor language, further inflected in matrix language</description>
         </category>
      </category>
   </annotation-set>
